<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.pojo.UrineTest" table="urine_test">
<id name="uid" column="uid"></id>
<property name="pat_id" column="pat_id"></property>
<property name="patient_name" column="patient_name"></property>
<property name="patient_gender" column="patient_gender"></property>
<property name="date_of_birth" column="date_of_birth"></property>
<property name="patient_phone" column="patient_phone"></property>
<property name="test_type" column="test_type"></property>
<property name="referred_by_doctor" column="referred_by_doctor"></property>
<property name="date_and_time" column="date_and_time"></property>
<property name="color" column="color"></property>
<property name="appearance" column="appearance"></property>
<property name="specific_gravity" column="specific_gravity"></property>
<property name="ph" column="ph"></property>
<property name="protein" column="protein"></property>
<property name="glucose" column="glucose"></property>
<property name="urobilinogen" column="urobilinogen"></property>
<property name="bilirubin" column="bilirubin"></property>
<property name="ketones" column="ketones"></property>
<property name="nitrites" column="nitrites"></property>
<property name="status" column="status"></property>
</class>
</hibernate-mapping>