<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.Lotus.StudentDetails" table="Student">
<id name="id" column="sid"></id>
<property name="name" column="sname"></property>
<property name="marks" column ="marks"></property>
<property name="address" column ="address"></property>
<property name="parent_name" column ="parent_name"></property>
</class>
</hibernate-mapping> 